{
    "name": "joelharkes/laravel-strict-validation",
    "description": "Typesafe validation rules for laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Joelharkes\\LaravelStrictValidation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joelharkes\\LaravelStrictValidation\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "joelharkes",
            "email": "jlharkes@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/validation": "^10.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpunit/phpunit": "^9.0"
    },
    "scripts" : {
        "lint" : "php-cs-fixer fix -vvv"
    }
}
